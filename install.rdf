<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id>mp4downloader@jeff.net</em:id>
        <em:version>1.4.0a1</em:version>
        <em:type>2</em:type>
        <em:bootstrap>true</em:bootstrap>
        
        <em:name>MP4 Downloader</em:name>
        <em:creator>Jake Hartz</em:creator>
        <!-- <em:translator>translator name</em:translator> -->
        <!-- <em:translator>another translator name</em:translator> -->
        
        <em:localized>
            <Description>
                <em:locale>en-US</em:locale>
                <em:name>MP4 Downloader</em:name>
                <em:description>Download MP4 videos from YouTube, Dailymotion, and Vimeo</em:description>
            </Description>
        </em:localized>
        
        <!-- Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>29.0</em:minVersion>
                <!-- (compatible by default since Firefox 10) -->
                <em:maxVersion>44.0</em:maxVersion>
            </Description>
        </em:targetApplication>
        
        <!-- SeaMonkey -->
        <em:targetApplication>
            <Description>
                <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
                <em:minVersion>2.26</em:minVersion>
                <!-- (compatible by default since SeaMonkey 2.7) -->
                <em:maxVersion>2.39</em:maxVersion>
            </Description>
        </em:targetApplication>
        
        <em:iconURL>chrome://mp4downloader/content/logo.png</em:iconURL>
        <em:optionsURL>chrome://mp4downloader/content/preferences.xul</em:optionsURL>
        <em:homepageURL>http://jhartz.github.io/mp4downloader/</em:homepageURL>
        
        <!-- Screw it, with extension signing, we'll just push updates thru AMO :/ -->
        <!-- <em:updateURL>https://www.mozdev.org/p/updates/mp4downloader/mp4downloader@jeff.net/update.rdf</em:updateURL> -->
    </Description>
</RDF>
